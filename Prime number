#include<stdio.h>
int main(){
 int n,count =0;
 printf("Enter a number: ");
 scanf("%d",&n);
 if (n==0 || n==1){
   printf("%d is not prime number");
 }else{
  for(int i=2;i<=n;i++){
    if (n%i==0){
      count=1;
      break;
     }
  }
  if(count==0){
   printf("Number is prime");
  }
  else{
   printf("number is not prime");
  }
 }
return 0;



